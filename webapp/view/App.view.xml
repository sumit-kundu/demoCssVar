<mvc:View controllerName="com.DemoCSSVar.controller.App" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:ce="sap.ui.codeeditor"
	height="100%">
	<IconTabHeader id="iconTabHeader" selectedKey="invalidKey" select="onSelectTab">
		<items>
			<IconTabFilter text="A" key="A"></IconTabFilter>
			<IconTabFilter text="B" key="B"></IconTabFilter>
		</items>
	</IconTabHeader>
	<Bar>
		<contentRight>
			<Label text="Dark Mode"/>
			<Switch state="false" change=".onChangeMode">
				<layoutData>
					<FlexItemData growFactor="1"/>
				</layoutData>
			</Switch>
		</contentRight>
	</Bar>
	<ce:CodeEditor id="aCodeEditor" class="myEditor" height="300px" type="javascript"/>
</mvc:View>